# Backend Staging Environment Variables
# Copy this file to .env.staging and fill in your staging environment values

# Google Cloud Credentials (Staging)
# For Render.com deployment, set GOOGLE_CREDENTIALS as environment variable
# For local development, place the JSON file in backend/keys/ directory
GOOGLE_CREDENTIALS=

# Gemini API Key (Staging)
GEMINI_API_KEY=your_staging_gemini_api_key

# Flask Configuration
FLASK_ENV=staging
FLASK_DEBUG=False

# CORS Origins (Staging)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,https://michela-staging.vercel.app

# Environment identifier
ENVIRONMENT=staging
